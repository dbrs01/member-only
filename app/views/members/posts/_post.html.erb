<div id="<%= dom_id post %>">
  <p>
    <strong>Title:</strong>
    <%= post.title %>
  </p>

  <p>
    <strong>Contents:</strong>
    <%= post.contents %>
  </p>

  <p>
    <strong>Member:</strong>
    <%= post.member_id %>
  </p>

  <p>
    <%= link_to "Show this post", [@member, post] %>

  </p>

</div>
